{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb", "airbnb-typescript"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "overrides": [],
  "rules": {
    "semi": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "no-console": "off",
    "no-return-assign": "off",
    "no-param-reassign": "off",
  },
  "plugins": ["react"]
}
